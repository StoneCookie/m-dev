      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:gravity="center"
      android:orientation="vertical"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
      tools:layout_editor_absoluteX="0dp">


    <LinearLayout
        android:layout_width="@dimen/dp_344"
        android:layout_height="@dimen/dp_178"
        android:layout_marginTop="@dimen/dp_16"
        android:paddingHorizontal="@dimen/dp_16"
        android:paddingTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_12"
        android:background="@drawable/shape"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_64"
            android:orientation="horizontal">

          <ImageView
              android:id="@+id/imageView5"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginEnd="@dimen/dp_16"
              android:layout_marginTop="@dimen/dp_12"
              android:contentDescription="@string/avatar"
              app:srcCompat="@drawable/ic_man_1" />

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/roboto"
                android:text="@string/name_1" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/date_1" />
          </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:fontFamily="@font/roboto"
            android:text="@string/description_1" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

          <TextView
              android:id="@+id/textView7"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:layout_marginTop="@dimen/dp_8"
              android:layout_weight="1"
              android:fontFamily="@font/roboto"
              android:textColor="@color/blue"
              android:text="@string/replays" />

          <ImageButton
              android:id="@+id/imageButton"
              android:layout_width="@dimen/dp_24"
              android:layout_height="@dimen/dp_24"
              android:layout_marginTop="@dimen/dp_4"
              android:background="none"
              android:contentDescription="@string/like"
              app:srcCompat="@drawable/ic_like" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="@dimen/dp_344"
        android:layout_height="@dimen/dp_159"
        android:layout_marginVertical="@dimen/dp_16"
        android:paddingHorizontal="@dimen/dp_16"
        android:paddingTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_12"
        android:background="@drawable/shape"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_64"
            android:orientation="horizontal">

          <ImageView
              android:id="@+id/imageView7"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginHorizontal="@dimen/dp_16"
              android:layout_marginTop="@dimen/dp_12"
              android:contentDescription="@string/avatar"
              app:srcCompat="@drawable/ic_woman_1" />

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">


            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/roboto"
                android:text="@string/name_2" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:text="@string/date_2" />
          </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:fontFamily="@font/roboto"
            android:text="@string/description_2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

          <TextView
              android:id="@+id/textView14"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:layout_marginTop="@dimen/dp_8"
              android:layout_weight="1"
              android:fontFamily="@font/roboto"
              android:textColor="@color/blue"
              android:text="@string/replays" />

          <ImageButton
              android:id="@+id/imageButton2"
              android:layout_width="@dimen/dp_24"
              android:layout_height="@dimen/dp_24"
              android:layout_marginTop="@dimen/dp_4"
              android:background="none"
              android:contentDescription="@string/like"
              app:srcCompat="@drawable/ic_like" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="@dimen/dp_344"
        android:layout_height="@dimen/dp_159"
        android:layout_marginBottom="@dimen/dp_64"
        android:paddingHorizontal="@dimen/dp_16"
        android:paddingTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_12"
        android:background="@drawable/shape"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_64"
            android:orientation="horizontal">

          <ImageView
              android:id="@+id/imageView8"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginHorizontal="@dimen/dp_16"
              android:layout_marginTop="@dimen/dp_12"
              android:contentDescription="@string/avatar"
              app:srcCompat="@drawable/ic_man_2" />

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">


            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/roboto"
                android:text="@string/name_3" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:text="@string/date_1" />
          </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:fontFamily="@font/roboto"
            android:text="@string/description_3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

          <TextView
              android:id="@+id/textView15"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:layout_marginTop="@dimen/dp_8"
              android:layout_weight="1"
              android:fontFamily="@font/roboto"
              android:textColor="@color/blue"
              android:text="@string/replays" />

          <ImageButton
              android:id="@+id/imageButton3"
              android:layout_width="@dimen/dp_24"
