      android:layout_height="@dimen/dp_56"
      android:orientation="horizontal"
      app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
      tools:layout_editor_absoluteX="0dp"
      android:baselineAligned="false">

    <LinearLayout
        android:layout_width="@dimen/dp_72"
        android:layout_height="@dimen/dp_43"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_weight="1"
        android:orientation="vertical">

      <ImageButton
          android:id="@+id/imageButton9"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/dp_2"
          app:srcCompat="@drawable/ic_shopping"
          android:background="none"
          android:contentDescription="@string/shopping" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="@dimen/dp_72"
        android:layout_height="@dimen/dp_43"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_weight="1"
        android:orientation="vertical">

      <ImageButton
          android:id="@+id/imageButton10"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/dp_2"
          app:srcCompat="@drawable/ic_book_open_variant"
          android:background="none"
          android:contentDescription="@string/book" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="@dimen/dp_72"
        android:layout_height="@dimen/dp_43"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_weight="1"
        android:orientation="vertical">

      <ImageButton
          android:id="@+id/imageButton11"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/dp_2"
          app:srcCompat="@drawable/ic_cart"
          android:background="none"
          android:contentDescription="@string/cart" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="@dimen/dp_72"
        android:layout_height="@dimen/dp_43"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_weight="1"
        android:orientation="vertical">

      <ImageButton
          android:id="@+id/imageButton12"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/dp_2"
          app:srcCompat="@drawable/ic_tag_heart"
          android:background="none"
          android:contentDescription="@string/tag_heart" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="@dimen/dp_72"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

      <ImageButton
          android:id="@+id/imageButton13"
          android:layout_width="match_parent"
          android:layout_height="@dimen/dp_24"
          android:layout_marginTop="@dimen/dp_6"
          app:srcCompat="@drawable/ic_heart_filled"
          android:background="none"
          android:contentDescription="@string/heart_filled" />

      <TextView
          android:id="@+id/textView19"
          android:layout_width="match_parent"
          android:layout_height="@dimen/dp_16"
          android:gravity="center"
          android:textColor="@color/blue"
          android:fontFamily="@font/roboto"
          android:text="@string/account" />
    </LinearLayout>

  </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>